{
    "name": "upload-build-action",
    "version": "1.0.0",
    "description": "Upload build artifacts to Nunu.ai",
    "main": "dist/index.js",
    "scripts": {
        "build": "ncc build src/main.ts -o dist --source-map --minify",
        "format": "prettier --write '**/*.ts'",
        "lint": "eslint src/**/*.ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nunu-ai/upload-build-action.git"
    },
    "keywords": [
        "actions",
        "nunu",
        "testing",
        "upload",
        "build"
    ],
    "author": "Nunu.ai",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.10.1",
        "@actions/exec": "^1.1.1",
        "@actions/tool-cache": "^2.0.1",
        "@actions/http-client": "^2.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@vercel/ncc": "^0.38.1",
        "typescript": "^5.3.3",
        "prettier": "^3.1.0",
        "eslint": "^8.55.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@typescript-eslint/eslint-plugin": "^6.14.0"
    }
}